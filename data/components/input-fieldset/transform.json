{
  "fieldset": [
    {
      "$if": [
        { "$value": "isDisabled" },
        { "attr": [ "disabled", "" ] }
      ]
    },
    {
      "$if": [
        { "$value": "fieldsetData" },
        { "dataset": { "$value": "fieldsetData" } }
      ]
    },
    { "addClasses": { "$value": "fieldsetClasses" } }
  ]
}