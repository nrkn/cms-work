djust-lightness,\n    $adjust-saturation\n  );\n\n  $border-highlight: advanced-lighten(\n    $base-color,\n    $adjust-hue,\n    $adjust-lightness,\n    $adjust-saturation\n  );\n  $border-shadow: advanced-darken(\n    $gradient-shadow,\n    $adjust-hue,\n    $adjust-lightness,\n    $adjust-saturation\n  );\n\n  $text-shadow: advanced-darken(\n    $border-shadow,\n    $adjust-hue,\n    $adjust-lightness,\n    $adjust-saturation\n  );\n\n  @include colorize-el-background( $base-color );\n  border: 0.0625rem solid $border-highlight;\n  border-bottom: 0.0625rem solid $border-shadow;\n  border-right: 0.0625rem solid $border-shadow;\n  text-shadow: 0.0625rem 0.0625rem 0 $text-shadow;\n}"},"templates":{"composer":{"value":{"nodeType":"documentFragment","_id":"documentfragment-890bd47485a01ebec4d8cefd0f56f7a5"},"children":[{"value":{"nodeType":"element","tagName":"div","attributes":{"class":"composer"},"_id":"element-77259b1653001df0b497fe5acfd7c966"},"children":[]}]},"composer-node":{"value":{"nodeType":"documentFragment","_id":"documentfragment-ffec14723f121f878ce12d31c9263125"},"children":[{"value":{"nodeType":"element","tagName":"div","attributes":{"class":"composer-node","data-tag":"nodeTag"},"_id":"element-7e39e154b0e1729167bcb4eb93db096c"},"children":[{"value":{"nodeType":"text","nodeValue":"\r\n  ","_id":"text-e7244bcc0661d981cbe6d7b073d13d09"},"children":[]},{"value":{"nodeType":"element","tagName":"header","attributes":{"class":"composer-node__toolbar"},"_id":"element-406448f0dde0868db550ad6190c71b1e"},"children":[{"value":{"nodeType":"text","nodeValue":"\r\n    ","_id":"text-97402da0ba281653a801d303589affb3"},"children":[]},{"value":{"nodeType":"element","tagName":"span","attributes":{"class":"composer-node__title","data-text":"title"},"_id":"element-f8e1737decd3829140aa7145276aaea3"},"children":[]},{"value":{"nodeType":"text","nodeValue":"\r\n    ","_id":"text-545a5affd8c3d3b458e146f39101d0e8"},"children":[]},{"value":{"nodeType":"element","tagName":"ul","attr